@article{Gibson2013a,
	title = {Rational integration of noisy evidence and prior semantic expectations in sentence interpretation.},
	volume = {110},
	issn = {1091-6490},
	url = {http://www.pnas.org/content/110/20/8051.full%5Cnhttp://www.scopus.com/inward/record.url?eid=2-s2.0-84877862479&partnerID=tZOtx3y1},
	doi = {10.1073/pnas.1216438110},
	abstract = {Sentence processing theories typically assume that the input to our language processing mechanisms is an error-free sequence of words. However, this assumption is an oversimplification because noise is present in typical language use (for instance, due to a noisy environment, producer errors, or perceiver errors). A complete theory of human sentence comprehension therefore needs to explain how humans understand language given imperfect input. Indeed, like many cognitive systems, language processing mechanisms may even be "well designed"--in this case for the task of recovering intended meaning from noisy utterances. In particular, comprehension mechanisms may be sensitive to the types of information that an idealized statistical comprehender would be sensitive to. Here, we evaluate four predictions about such a rational (Bayesian) noisy-channel language comprehender in a sentence comprehension task: (i) semantic cues should pull sentence interpretation towards plausible meanings, especially if the wording of the more plausible meaning is close to the observed utterance in terms of the number of edits; (ii) this process should asymmetrically treat insertions and deletions due to the Bayesian "size principle"; such nonliteral interpretation of sentences should (iii) increase with the perceived noise rate of the communicative situation and (iv) decrease if semantically anomalous meanings are more likely to be communicated. These predictions are borne out, strongly suggesting that human language relies on rational statistical inference over a noisy channel.},
	number = {20},
	journal = {Proceedings of the National Academy of Sciences of the United States of America},
	author = {Gibson, Edward and Bergen, Leon and Piantadosi, Steven T.},
	year = {2013},
	pmid = {23637344},
	note = {ISBN: 1216438110},
	keywords = {Humans, Language, Cognition, Comprehension, Semantics, Psycholinguistics, communication, Communication, psycholinguistics, Bayes Theorem, Natural Language Processing, rational inference, Reproducibility of Results},
	pages = {8051--6},
	file = {Gibson, Bergen, Piantadosi - 2013 - Rational integration of noisy evidence and prior semantic expectations in sentence interpretation:C\:\\Users\\fabianmoss\\Zotero\\storage\\WCJUPQ63\\Gibson, Bergen, Piantadosi - 2013 - Rational integration of noisy evidence and prior semantic expectations in sentence interpretation.pdf:application/pdf}
}

@article{Fedorenko2014,
	title = {The {P}600 {ERP} component indexes rational error correction within a noisy-channel model of human communication},
	issn = {13514180},
	doi = {10.1007/s13398-014-0173-7.2},
	abstract = {The traditional interpretation of the P600 ERP component as an index of syntactic integration difficulty has been challenged on both theoretical and empirical grounds. We propose and evaluate a novel account of the P600 within a rational inference approach to sentence comprehension. In particular, we argue that the P600 indexes error correction and ensues whenever the probability that the received message was corrupted by noise (due to production or perception errors) exceeds the probability that it was produced intentionally and perceived accurately. We report an ERP experiment that shows that semantic violations that can be explained by a plausible noise process elicit a P600 effect. For example, in a sentence like “The storyteller could turn any incident into an amusing antidote”, the intended word (“anecdote”) is recoverable because it is phonologically and orthographically close to antidote, and is thus a likely erroneous substitution.},
