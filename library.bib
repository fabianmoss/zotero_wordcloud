	author = {Bader, Rolf},
	month = oct,
	year = {2014},
	note = {Publisher: Acoustical Society of America},
	pages = {2163--2163}
}

@article{Bittencourt2013,
	title = {Reimagining a {Riemannian} symbology for the structural harmonic analysis of 19th-century tonal music.},
