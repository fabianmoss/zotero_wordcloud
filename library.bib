@article{jimenez_connecting_2017,
	title = {Connecting chord progressions with specific pieces of music},
	issn = {0305-7356},
	url = {http://journals.sagepub.com/doi/10.1177/0305735617721638},
	doi = {10.1177/0305735617721638},
	abstract = {Musicians can conceptualize harmony in terms of its connection to specific pieces of music. However, research appears to indicate that harmony plays a relatively unimportant role in music identification tasks. The present study examines the ability of listeners of varying levels of musical expertise to identify music from chord progressions. Participants were asked to identify well-known classical and pop/rock pieces from their chord progressions, which were recorded using either piano tones or Shepard tones and were played at six transpositional levels. Although musical training and invariance of surface melodic and rhythmic features were found to have an advantageous effect on the identification task, even some non-musicians were able to identify music from chord progressions in conditions of low invariance of surface features. Implications of these results for our understanding of how listeners mentally represent and remember harmony are discussed.},
	urldate = {2017-09-25},
	journal = {Psychology of Music},
	author = {Jimenez, Ivan and Kuusi, Tuire},
	month = aug,
	year = {2017},
	note = {Publisher: SAGE PublicationsSage UK: London, England},
	keywords = {harmony, musical memory, aural skills, memory for harmony, music identification},
