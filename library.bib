@article{chew_thinking_2003,
	title = {Thinking out of the grid and inside the spiral-{Geometric} interpretations of and comparisons with the spiral array model.},
	abstract = {This paper presents a geometric interpretation of the Spiral Array model and its comparison to Lerdahl's Tonal Pitch Space and Krumhansl's spatial representation of pitch relations. The Spiral Array model is based on the Harmonic Network. The fundamental idea underlying the model is the representing of higher level objects in the spiral's interior as convex combinations of the representations of the representations of lower level components. By using the interior of the spiral, the original discrete space is relaxed to one that is continuous. Geometric mappings are demonstrated among Lerdahl's Tonal Pitch Space, Krumhansl's spatial representation of pitch relations and the Spiral Array model. The interior point approach of the Spiral Array model is shown to generate higher level structures that consistent with the results of these other approaches. The advantage of the interior point approach is that it facilitates comparisons across different hierarchical levels and problems that were previously combinatorial in nature and can be modelled more efficiently using the continuous space in the interior.},
	journal = {Technical Report 03-002},
	author = {Chew, Elaine},
	year = {2003},
	file = {Attachment:C\:\\Users\\fabianmoss\\Zotero\\storage\\VGGK2W8B\\Chew_2003_Thinking out of the grid and inside the spiral-Geometric interpretations of and comparisons with the spiral array model.pdf:application/pdf;Attachment:C\:\\Users\\fabianmoss\\Zotero\\storage\\A3EB5NNT\\Chew - 2003 - Thinking out of the grid and inside the spiral-Geometric interpretations of and comparisons with the spiral array model.pdf:application/pdf;Chew - 2003 - Thinking out of the grid and inside the spiral-Geometric interpretations of and comparisons with the spiral array model:C\:\\Users\\fabianmoss\\Zotero\\storage\\N5647K7S\\Chew - 2003 - Thinking out of the grid and inside the spiral-Geometric interpretations of and comparisons with the spiral array model.pdf:application/pdf}
