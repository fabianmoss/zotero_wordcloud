	keywords = {pythagoras, euclid, Euclid’S Elements, Golden Section, hypothesis, Mathematics of Harmony, Plato, Platonic Solids, proclus, Proclus’ Hypothesis, s elements, Pythagoras, golden section, mathematics of harmony, plato, platonic solids},
