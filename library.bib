	keywords = {Humans, Learning, Learning: physiology, Music, Cognition, Cognition: physiology, Computer Simulation, Pitch Discrimination, Aptitude, Aptitude: physiology, Pitch Discrimination: physiology, Nerve Net, Nerve Net: physiology, Awareness},
