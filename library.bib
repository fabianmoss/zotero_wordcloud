	keywords = {Humans, Music, Adult, Auditory Perception, Evoked Potentials, Acoustic Stimulation, Mental Processes, Pitch Discrimination, Female, Male, Auditory, Analysis of Variance},
	pages = {1545--1554},
	file = {Poulin-Charronnat, Bigand, Koelsch - 2006 - Processing of musical syntax tonic versus subdominant an event-related potential study:C\:\\Users\\fabianmoss\\Zotero\\storage\\DPSCDGAY\\Poulin-Charronnat, Bigand, Koelsch - 2006 - Processing of musical syntax tonic versus subdominant an event-related potential study.pdf:application/pdf}
