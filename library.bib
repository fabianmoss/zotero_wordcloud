@article{du_sequential_2012,
	title = {Sequential latent {Dirichlet} allocation},
	volume = {31},
	issn = {02191377},
	doi = {10.1007/s10115-011-0425-1},
	abstract = {In recent years, the language model Latent Dirichlet Allocation (LDA),{\textbackslash}nwhich clusters co-occurring words into topics, has been widely appled{\textbackslash}nin the computer vision field. However, many of these applications{\textbackslash}nhave difficulty with modeling the spatial and temporal structure{\textbackslash}namong visual words, since LDA assumes that a document is a “bag-of-words”.{\textbackslash}nIt is also critical to properly design “words” and “documents” when{\textbackslash}nusing a language model to solve vision problems. In this pa- per,{\textbackslash}nwe propose a topic model Spatial Latent Dirichlet Allocation (SLDA),{\textbackslash}nwhich better encodes spatial structure among visual words that are{\textbackslash}nessential for solving many vision problems. The spatial information{\textbackslash}nis not encoded in the value of visual words but in the design of{\textbackslash}ndocuments. Instead of knowing the partition of words into documents{\textbackslash}na priori, the word-document assignment becomes a random hidden variable{\textbackslash}nin SLDA. There is a generative procedure, where knowledge of spatial{\textbackslash}nstructure can be flexibly added as a prior, grouping visual words{\textbackslash}nwhich are close in space into the same document. We use SLDA to discover{\textbackslash}nobjects from a collection of images, and show it achieves better{\textbackslash}nperformance than LDA.},
	number = {3},
	journal = {Knowledge and Information Systems},
	author = {Du, Lan and Buntine, Wray and Jin, Huidong and Chen, Changyou},
	year = {2012},
	note = {ISBN: 1011501104},
	keywords = {Collapsed Gibbs sampler, Document structure, Latent Dirichlet allocation, Poisson-Dirichlet process, Topic model},
	pages = {475--503}
}

