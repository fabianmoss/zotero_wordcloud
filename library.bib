@article{Bailes2013,
	title = {Music {Cognition} as {Mental} {Time} {Travel}},
	volume = {3},
	issn = {2045-2322},
	url = {http://www.nature.com/doifinder/10.1038/srep02690},
	doi = {10.1038/srep02690},
	abstract = {As we experience a temporal flux of events our expectations of future events change. Such expectations seem
to be central to our perception of affect in music, but we have little understanding of how expectations
change as recent information is integrated. When music establishes a pitch centre (tonality), we rapidly learn
to anticipate its continuation. What happens when anticipations are challenged by new events? Here we
show that providing a melodic challenge to an established tonality leads to progressive changes in the impact
of the features of the stimulus on listeners’ expectations. The results demonstrate that retrospective analysis
of recent events can establish new patterns of expectation that converge towards probabilistic
interpretations of the temporal stream. These studies point to wider applications of understanding the
impact of information flow on future prediction and its behavioural utility.},
	journal = {Scientific Reports},
	author = {Bailes, Freya and Dean, Roger T. and Pearce, Marcus T.},
	month = sep,
	year = {2013},
	file = {Attachment:C\:\\Users\\fabianmoss\\Zotero\\storage\\MNQ9Z4QT\\Bailes, Dean, Pearce_2013_Music Cognition as Mental Time Travel.pdf:application/pdf;Attachment:C\:\\Users\\fabianmoss\\Zotero\\storage\\Z8MXFQIU\\Bailes, Dean, Pearce - 2013 - Music Cognition as Mental Time Travel.pdf:application/pdf;Bailes, Dean, Pearce - 2013 - Music Cognition as Mental Time Travel:C\:\\Users\\fabianmoss\\Zotero\\storage\\H9H4ZBL4\\Bailes, Dean, Pearce - 2013 - Music Cognition as Mental Time Travel.pdf:application/pdf;PDF:C\:\\Users\\fabianmoss\\Zotero\\storage\\JYWY5HQ8\\Bailes, Dean, Pearce - 2013 - Music Cognition as Mental Time Travel.pdf:application/pdf}
