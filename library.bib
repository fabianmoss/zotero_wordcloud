@article{christiansen_language_2008,
	title = {Language as shaped by the brain.},
	volume = {31},
	issn = {1469-1825},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/18826669},
	doi = {10.1017/S0140525X08004998},
	abstract = {It is widely assumed that human learning and the structure of human languages are intimately related. This relationship is frequently suggested to derive from a language-specific biological endowment, which encodes universal, but communicatively arbitrary, principles of language structure (a Universal Grammar or UG). How might such a UG have evolved? We argue that UG could not have arisen either by biological adaptation or non-adaptationist genetic processes, resulting in a logical problem of language evolution. Specifically, as the processes of language change are much more rapid than processes of genetic change, language constitutes a "moving target" both over time and across different human populations, and, hence, cannot provide a stable environment to which language genes could have adapted. We conclude that a biologically determined UG is not evolutionarily viable. Instead, the original motivation for UG–the mesh between learners and languages–arises because language has been shaped to fit the human brain, rather than vice versa. Following Darwin, we view language itself as a complex and interdependent "organism," which evolves under selectional pressures from human learning and processing mechanisms. That is, languages themselves are shaped by severe selectional pressure from each generation of language users and learners. This suggests that apparently arbitrary aspects of linguistic structure may result from general learning and processing biases deriving from the structure of thought processes, perceptuo-motor factors, cognitive limitations, and pragmatics.},
	number = {5},
	journal = {The Behavioral and brain sciences},
	author = {Christiansen, Morten H and Chater, Nick},
	month = oct,
	year = {2008},
	pmid = {18826669},
	keywords = {Humans, Child, Linguistics, Cognition, Cross-Cultural Comparison, Comprehension, Semantics, Brain, Adaptation, Biological, Biological Evolution, Brain: physiology, Cognition: physiology, Comprehension: physiology, Genetic, Instinct, Language Development, Selection, Social Environment, Selection, Genetic, *Biological Evolution, *Language Development, Adaptation, Biological, Brain/*physiology, Cognition/physiology, Comprehension/physiology},
	pages = {489--508; discussion 509--58},
	file = {Attachment:C\:\\Users\\fabianmoss\\Zotero\\storage\\PVGUESMI\\Christiansen, Chater_2008_Language as shaped by the brain.pdf:application/pdf;Attachment:C\:\\Users\\fabianmoss\\Zotero\\storage\\7H7FSSRH\\Christiansen, Chater_2008_Language as shaped by the brain.pdf:application/pdf;Attachment:C\:\\Users\\fabianmoss\\Zotero\\storage\\2U577MX9\\Christiansen, Chater_2008_Language as shaped by the brain.pdf:application/pdf;Christiansen, Chater - 2008 - Language as shaped by the brain:C\:\\Users\\fabianmoss\\Zotero\\storage\\FC2QPF56\\Christiansen, Chater - 2008 - Language as shaped by the brain.pdf:application/pdf}
}

@article{seidenberg_language_1997,
	title = {Language acquisition and use: learning and applying probabilistic constraints.},
	volume = {275},
	issn = {0036-8075},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/9054348},
	abstract = {What kinds of knowledge underlie the use of language and how is this knowledge acquired? Linguists equate knowing a language with knowing a grammar. Classic "poverty of the stimulus" arguments suggest that grammar identification is an intractable inductive problem and that acquisition is possible only because children possess innate knowledge of grammatical structure. An alternative view is emerging from studies of statistical and probabilistic aspects of language, connectionist models, and the learning capacities of infants. This approach emphasizes continuity between how language is acquired and how it is used. It retains the idea that innate capacities constrain language learning, but calls into question whether they include knowledge of grammatical structure.},
	number = {5306},
	journal = {Science (New York, N.Y.)},
	author = {Seidenberg, M S},
	month = mar,
	year = {1997},
	pmid = {9054348},
	keywords = {Algorithms, Humans, Learning, Child, Linguistics, Language Development, Adult, Infant, Neural Networks (Computer), Probability, Psycholinguistics},
	pages = {1599--603},
