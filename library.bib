@article{fiore_commuting_2011,
	title = {Commuting groups and the topos of triads},
	url = {http://arxiv.org/abs/1102.1496},
	abstract = {The goal of this article is to clarify the relationship between the topos of triads and the neo-Riemannian PLR-group. To do this, we first develop some theory of generalized interval systems: 1) we prove the well known fact that every pair of dual groups is isomorphic to the left and right regular representations of some group (Cayley's Theorem), 2) given a simply transitive group action, we show how to construct the dual group, and 3) given two dual groups, we show how to easily construct sub dual groups. Examples of this construction of sub dual groups include Cohn's hexatonic systems, as well as the octatonic systems. We then enumerate all Z\_\{12\}-subsets which are invariant under the triadic monoid and admit a simply transitive PLR-subgroup action on their maximal triadic covers. As a corollary, we realize all four hexatonic systems and all three octatonic systems as Lawvere–Tierney upgrades of consonant triads.},
	journal = {Springer},
	author = {Fiore, Thomas M. and Noll, Thomas},
	month = feb,
	year = {2011},
	keywords = {NRT},
	file = {Attachment:C\:\\Users\\fabianmoss\\Zotero\\storage\\U89CWR76\\Fiore, Noll - 2011 - Commuting groups and the topos of triads.pdf:application/pdf;Attachment:C\:\\Users\\fabianmoss\\Zotero\\storage\\3F3RN339\\Fiore, Noll_2011_Commuting groups and the topos of triads.pdf:application/pdf;Fiore, Noll - 2011 - Commuting groups and the topos of triads:C\:\\Users\\fabianmoss\\Zotero\\storage\\38L7MWIY\\Fiore, Noll - 2011 - Commuting groups and the topos of triads.pdf:application/pdf}
}

@article{frankland_key-finding_2012,
	title = {Key-{Finding} {Using} the {Krumhansl} and {Schmuckler} {Effects} of the to {Tonality} in the {Algorithm} {Quantify} {Task} {Pitch}-{Comparison}},
	volume = {14},
	abstract = {Musicians and nonmusicians listened to musical phrases that were either selected from the classical repertoire or composed for the experiments. The phrases ended either congruously or with a nondiatonic, diatonic, or rhythmic violation. Percentage of correct responses was analyzed in Experiment 1, and event-related potentials (ERPs) were recorded in Experiments 2 and 3. Musicians performed better than nonmusicians in recognizing familiar musical phrases and classifying terminal notes. The differences found as a function of expertise were larger for unfamiliar than for familiar melodies. The ERPs to the end notes differed both in terms of amplitude and latency between musicians and nonmusicians, and as a function of participants' familiarity with the melodies and type of violation. Results show that expertise influences the decisional rather than the purely perceptual aspects of music processing and that ERPs can provide important insight into the study of music perception.},
