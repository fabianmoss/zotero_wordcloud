	keywords = {Humans, Music, Language, Brain, Brain: physiology, Auditory Perception, Auditory Perception: physiology, Magnetic Resonance Imaging, Mental Processes, Mental Processes: physiology, Female, Male, Aphasia, Aphasia: diagnosis, Brain Mapping, Electrophysiology, Electrophysiology: methods, Magnetic Resonance Imaging: methods, Psychological Theory},
