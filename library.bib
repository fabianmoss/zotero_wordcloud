	author = {Sammler, Daniela and Koelsch, Stefan and Ball, T. and Brandt, A. and Elger, C. E. and Friederici, A. D. and Grigutsch, M. and Huppertz, H. J. and Knösche, T. R. and Wellmer, J. and Widman, G. and Schulze-Bonhage, A.},
